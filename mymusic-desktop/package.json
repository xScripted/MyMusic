  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "electron": "23.1.1",
    "electron-builder": "^23.6.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.12.1",
    "vue": "^3.2.47",
    "vue-tsc": "^1.1.7"
