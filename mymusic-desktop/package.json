{
  "name": "mymusic-desktop",
  "version": "1.0.5",
  "main": "dist-electron/main/index.js",
  "description": "MP3 Music Player for Windows!",
  "author": "Miquel Toran",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
